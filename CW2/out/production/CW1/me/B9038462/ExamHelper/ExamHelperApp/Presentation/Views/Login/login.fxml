<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.B9038462.ExamHelper.ExamHelperApp.Infrastructure.Application.Controllers.Login.LoginController">
   <children>
      <VBox alignment="CENTER_LEFT" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label text="Sign In">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin></Label>
            <Label text="Username" />
            <TextField fx:id="usernameTxt" />
            <Label text="Password" />
            <TextField fx:id="passwordTxt" />
            <Button fx:id="loginBtn" alignment="CENTER" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="299.0" text="Login">
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin></Button>
         </children>
         <padding>
            <Insets bottom="150.0" left="150.0" right="150.0" top="125.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
